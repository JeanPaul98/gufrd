    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "svgGenerator": {
    "outputPath": "./src/app/svg",
    "prefix": "faw",
    "srcPath": "./src/assets/svg",
    "svgoConfig": {
      "plugins": [
        "removeDimensions",
        "cleanupAttrs",
        {
          "name": "convertColors",
          "params": {
            "currentColor": true
          }
        }
      ]
    }
  },
