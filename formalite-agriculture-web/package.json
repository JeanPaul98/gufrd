    "build-csc": "ng build --project=csc",
    "build-cscp": "ng build --project=cscp",
    "build-ecp": "ng build --project=ecp",
    "build-ecpfl": "ng build --project=ecpfl",
    "build-iocav": "ng build --project=iocav",
    "build-ipocp": "ng build --project=ipocp",
